<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CreationUser</servlet-name>
  	<servlet-class>com.polito.tp1.servlets.CreationUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreationCommand</servlet-name>
  	<servlet-class>com.polito.tp1.servlets.CreationCommand</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreationUser</servlet-name>
  	<url-pattern>/CreationUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreationCommand</servlet-name>
  	<url-pattern>/CreationCommand</url-pattern>
  </servlet-mapping>
</web-app>
